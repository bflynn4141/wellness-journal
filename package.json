{
  "name": "wellness-journal",
  "version": "0.1.0",
  "description": "Daily wellness journal integrating Whoop, Google Calendar, and AI-powered reflections",
  "type": "module",
  "bin": {
    "wellness-journal": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "morning": "tsx src/index.ts morning",
    "setup": "tsx src/index.ts setup",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
