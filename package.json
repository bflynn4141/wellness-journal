    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "handlebars": "^4.7.8",
    "inquirer": "^12.1.0",
    "node-notifier": "^10.0.1",
    "open": "^10.1.0",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
